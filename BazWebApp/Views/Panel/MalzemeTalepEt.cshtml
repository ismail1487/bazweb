                                <select class="form-control select2" id="surecStatuBildirimTipiID" style="width: 100%;" required>
                                    <option value="">Seçiniz</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="form-group">
                                <label for="iadeEtNot">Not <span class="text-danger">*</span></label>
                                <textarea class="form-control" id="iadeEtNot" rows="3" placeholder="İade sebebi giriniz..." required></textarea>
                            </div>
